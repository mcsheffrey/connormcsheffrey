   HTML5 Boilerplate styles - h5bp.com (generated via initializr.com)
   ========================================================================== */

html,
button,
input,
select,
textarea {
    color: #222;
}

body {
    font-size: 1em;
    line-height: 1.4;
}

::-moz-selection {
    background: #b3d4fc;
    text-shadow: none;
}

::selection {
    background: #b3d4fc;
    text-shadow: none;
}

hr {
    display: block;
    height: 1px;
    border: 0;
    border-top: 1px solid #ccc;
    margin: 1em 0;
    padding: 0;
}

img {
    vertical-align: middle;
}

fieldset {
    border: 0;
    margin: 0;
    padding: 0;
}

textarea {
    resize: vertical;
}

.chromeframe {
    margin: 0.2em 0;
    background: #ccc;
    color: #000;
    padding: 0.2em 0;
}

/* ==========================================================================
   Author's custom styles
   ========================================================================== */

body {
  font-family: "proxima-nova", Helvetica, Arial, Sans-serif;
  font-size: 13px;
  line-height: 18px;
  color: #333333;
  -webkit-font-smoothing: antialiased;
  text-rendering: optimizeLegibility;
  padding-top: 130px;
}

body.blog {
  
}

body.work {
    text-align: center;
}

.masthead {
  position: absolute;
  max-width: auto;
  width: 100%;
}

.header-wrapper {
    left: 0;
    position: fixed;
    right: 0;
    top: 0;
    z-index: 200;
}

#header {
  background: rgba(255,255,255,.95);
    padding-top: 25px;
    padding-bottom: 25px;
    width: 100%;
    z-index: 100;

    #logo {
        display: inline-block;
        float: left;
        margin: 0;

        a {
            background: url(../img/connormcsheffrey.png);
            background-size: 100% auto;
            display: block;
            height: 80px;
            width: 80px;
        }
    }

    nav {
        float: right;
        margin: 30px 0 0 0;

        ul {
            list-style: none;
            margin: 0;
            padding: 0;

            li {
                float: left;
                margin: 0 20px 0 0;
                padding: 0 20px 0 0;
                position: relative;

                a {
                    color: #353535;
                    font-family: "proxima-nova", Helvetica, Arial, Sans-serif;
                    font-size: 2em;
                    font-weight: 200;
                    text-decoration: none;
                }

            }

            li.active a {
                text-decoration: line-through;
            }

            li:first-child:after {
                border-right: 1px solid #d6d6d6;
                content: "";
                display: block;
                height: 50px;
                font-size: 36px;
                margin: 0 0 20px 0;
                -webkit-transform: rotate(25deg);
                   -moz-transform: rotate(25deg);
                    -ms-transform: rotate(25deg);
                     -o-transform: rotate(25deg);
                        transform: rotate(25deg);
                position: absolute;
                right: 0;
                top: -15px;
            }

            li:last-child {
                margin: 0;
                padding: 0;
            }
        }

        ul:hover li a {
            text-decoration: none;

            &:hover {
                text-decoration: line-through;
            }
        }
    }
}

h2 {
    letter-spacing: .1em;
    margin: 0;
    text-transform: uppercase;
}

h3 {
    letter-spacing: .1em;
    margin: 0 0 20px 0;
    opacity: .4;
    text-transform: uppercase;
    line-height: 1.2em;
    font-size: 20px;
}

p {
    margin: 0 0 20px;
    line-height: 1.5em;
    font-size: 16px;
}

img {
    max-width: 100%;
}

@media only screen and (-webkit-min-device-pixel-ratio: 2) { 
  
  #header {

  #logo {

    a {
      background-image: url(../img/connormcsheffrey@2x.png);
    }
  }
}

}

#content {
  background: #fff;
  margin-top: 100%;
}

.content {
    margin-top: 40px;
}

.post-header {
    position: relative;
}

.post-header img {
    max-width: none;
    width: 100%;
}

.post-title {
  color: #fff;
  font-size: 100px;
  font-weight: 800;
  left: 0;
  letter-spacing: .1em;
  text-transform: uppercase;
  position: absolute;
  right: 0;
  top: 47%;
  text-align: center;
}

.hero-nav {
    border: 0;
    display: inline-block;
    height: 100%;
    position: absolute;
    opacity: 0;
    -webkit-transition: all 400ms ease-in-out;
       -moz-transition: all 400ms ease-in-out;
        -ms-transition: all 400ms ease-in-out;
         -o-transition: all 400ms ease-in-out;
            transition: all 400ms ease-in-out;
    top: 0;
    width: 10%;
    z-index: 1;
}

.post-header:hover .hero-nav {
    opacity: .8;
}

.hero-nav.hero-prev {
    background: -moz-linear-gradient(left, rgba(0,0,0,0.4) 0%, rgba(0,0,0,0) 100%); /* FF3.6+ */
    background: -webkit-gradient(linear, left top, right top, color-stop(0%,rgba(0,0,0,0.4)), color-stop(100%,rgba(0,0,0,0))); /* Chrome,Safari4+ */
    background: -webkit-linear-gradient(left, rgba(0,0,0,0.4) 0%,rgba(0,0,0,0) 100%); /* Chrome10+,Safari5.1+ */
    background: -o-linear-gradient(left, rgba(0,0,0,0.4) 0%,rgba(0,0,0,0) 100%); /* Opera 11.10+ */
    background: -ms-linear-gradient(left, rgba(0,0,0,0.4) 0%,rgba(0,0,0,0) 100%); /* IE10+ */
    background: linear-gradient(left, rgba(0,0,0,0.4) 0%,rgba(0,0,0,0) 100%); /* W3C */
    left: 0;
}

.hero-nav.hero-prev:hover {
    opacity: 1;
}

.hero-nav:after {
    border-bottom: 1px solid #D6D6D6;
    border-right: 1px solid #D6D6D6;
    content: '';
    display: inline-block;
    text-indent: -9999px;
    height: 50px;
    left: 25px;
    margin-top: -25px;
    top: 50%;
    position: absolute;
    -webkit-transform: rotate(135deg);
         -moz-transform: rotate(135deg);
            -ms-transform: rotate(135deg);
             -o-transform: rotate(135deg);
                    transform: rotate(135deg);
    width: 50px;
}

.hero-nav.hero-next {
    background: -moz-linear-gradient(left, rgba(0,0,0,0) 0%, rgba(0,0,0,0.4) 100%); /* FF3.6+ */
    background: -webkit-gradient(linear, left top, right top, color-stop(0%,rgba(0,0,0,0)), color-stop(100%,rgba(0,0,0,0.4))); /* Chrome,Safari4+ */
    background: -webkit-linear-gradient(left, rgba(0,0,0,0) 0%,rgba(0,0,0,0.4) 100%); /* Chrome10+,Safari5.1+ */
    background: -o-linear-gradient(left, rgba(0,0,0,0) 0%,rgba(0,0,0,0.4) 100%); /* Opera 11.10+ */
    background: -ms-linear-gradient(left, rgba(0,0,0,0) 0%,rgba(0,0,0,0.4) 100%); /* IE10+ */
    background: linear-gradient(left, rgba(0,0,0,0) 0%,rgba(0,0,0,0.4) 100%); /* W3C */
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#00000000', endColorstr='#4d000000',GradientType=1 ); /* IE6-9 */
    right: 0;
}

.hero-nav.hero-next:hover {
    opacity: 1;
}

.hero-nav.hero-next:after {
    left: auto;
    right: 25px;
    -webkit-transform: rotate(-45deg);
         -moz-transform: rotate(-45deg);
            -ms-transform: rotate(-45deg);
             -o-transform: rotate(-45deg);
                    transform: rotate(-45deg);
}

// Callouts yo

.additonal-posts .additonal-post {
    -webkit-transition: opacity 500ms ease 0s;
    -moz-transition: opacity 500ms ease 0s;
    -ms-transition: opacity 500ms ease 0s;
    -o-transition: opacity 500ms ease 0s;
    transition: opacity 500ms ease 0s;
}

.additonal-posts:hover .additonal-post {
    opacity: .6;
}

.additonal-posts:hover .additonal-post:hover {
    opacity: 1;
}

.additonal-post {
    position: relative;
    text-align: center;
}

.additonal-post h3 {
    color: #fff;
    position: absolute;
    top: 44%;
    left: 0;
    right: 0;
    opacity: 1;
    font-size: 34px;
}

.additonal-post img {
    width: 100%;
}

.fluidVideoWrapper {
    padding: 56.3% 0 0 0;
    width: 100%;
    position: relative;
}

.fluidVideoWrapper iframe, .fluidVideoWrapper object, .fluidVideoWrapper embed {
position: absolute;
top: 0;
left: 0;
width: 100%;
height: 100%;
}

.macbook {
    margin-bottom: 3em;
}

.odd {
    background: #EEEEEE;
}

.work-section {
    padding: 50px  0;
}

.work-section p:last-child {
    margin-bottom: 0;
}

footer {
    text-align: left;
}

.instagram-video {
    .post-header img {
        position: relative;
        z-index: 100;
    }

    .post-title {
        z-index: 101;
    }

    .fluidVideoWrapper {
        position: absolute;
        top: 0;
    }
}

.instagram-video .post-header:hover {
    .post-title {
        border-bottom: 8px solid #fff;
    }
}

